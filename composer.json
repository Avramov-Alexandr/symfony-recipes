{
    "name": "mp/recipes",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "MP\\CoreBundle\\": "mp/core-bundle/src/"
        }
    },
    "authors": [
        {
            "name": "Martin Muller",
            "email": "pr5@myworkplanner.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.0"
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://git.myworkplanner.net/mp-recipes/raw/b40ad93ef3ab1255ffa80c361447031165b2aa16/index.json",
                "flex://defaults"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.myworkplanner.net/mp-recipes.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
