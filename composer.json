{
    "name": "mp/core-bundle",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "MPCoreBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Martin Muller",
            "email": "pr5@myworkplanner.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.0"
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://raw.githubusercontent.com/Avramov-Alexandr/symfony-recipes/mid/index.json",
                "flex://defaults"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Avramov-Alexandr/symfony-recipes"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
