{
    "name": "mp/first-bundle",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Smalex\\SymfonyRecipes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Martin Muller",
            "email": "pr5@myworkplanner.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/flex": "^1.16"
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://raw.githubusercontent.com/Avramov-Alexandr/symfony-recipes/main/index.json",
                "flex://defaults"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Avramov-Alexandr/symfony-recipes"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
